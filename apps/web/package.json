{
  "name": "@org/web",
  "description": "Next.js instance",
  "version": "1.0.0",
  "private": true,
  "license": "MIT",
  "exports": {
    ".": {
      "serve": {
        "types": "./dist/index.js",
        "require": "./dist/index.js",
        "import": "./dist/index.js"
      },
      "default": {
        "types": "./index.ts",
        "require": "./index.ts",
        "import": "./index.ts"
      }
    },
    "./types": {
      "types": "./types.ts",
      "require": "./types.ts",
      "import": "./types.ts"
    }
  },
  "scripts": {
    "clean": "rm -rf node_modules dist build .next .turbo",
    "build:next": "next build",
    "build:cssVariables": "node ./scripts/write-css-variables.js",
    "build:server": "tsc --project tsconfig.server.json",
    "copyfiles:media": "copyfiles -u 1 media/**/*.{svg,jpg,png} dist/media",
    "build": "NODE_ENV=production pnpm build:server && pnpm copyfiles:media && pnpm build:cssVariables && pnpm build:next",
    "lint": "next lint"
  },
  "dependencies": {
    "@heroicons/react": "^2.0.18",
    "@org/cms": "workspace:*",
    "@org/shared": "workspace:*",
    "@org/uikit": "workspace:*",
    "autoprefixer": "10.4.14",
    "copyfiles": "^2.4.1",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "next": "13.4.6",
    "postcss": "8.4.24",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "^7.47.0",
    "react-intersection-observer": "^9.5.2",
    "react-scroll-parallax": "^3.4.2",
    "react-select": "^5.7.7",
    "tailwindcss": "3.3.2",
    "typescript": "5.1.3",
    "use-count-up": "^3.0.1"
  },
  "devDependencies": {
    "@org/eslint-config": "workspace:*",
    "@org/tailwind-config": "workspace:*",
    "@payloadcms/plugin-form-builder": "^1.0.15",
    "@types/express": "^4.17.17",
    "@types/node": "20.3.1",
    "@types/react": "18.2.12",
    "@types/react-dom": "18.2.5",
    "classnames": "^2.3.2",
    "eslint": "8.43.0",
    "eslint-config-next": "13.4.6",
    "nodemon": "^2.0.22",
    "ts-node": "^10.9.1"
  }
}
